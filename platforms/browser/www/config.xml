<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="68" id="app.fixchap.com" version="1.1.3" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>FixChap</name>
    <description>
        One-stop solution for all your home repair service needs.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        FixChap Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <universal-links>
        <host event="ul_myExampleEvent" name="fixchap.com" scheme="https">
            <path url="/mp/*" />
        </host>
    </universal-links>
    <preference name="ScrollEnabled" value="true" />
    <preference name="SplashScreenDelay" value="3000" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="phonegap-version" value="cli-9.0.0" />
    <preference name="Fullscreen" value="false" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarBackgroundColor" value="#11978E" />
    <preference name="StatusBarDefaultScrollToTop" value="true" />
    <preference name="BackgroundColor" value="0xff0000ff" />
    <preference name="Orientation" value="portrait" />
    <preference name="AndroidXEnabled" value="true" />
    <preference name="GradlePluginKotlinEnabled" value="true" />
    <preference name="GOOGLE_MAPS_ANDROID_API_KEY" value="AIzaSyD-T2yF06JC5U_8Lou45QeKVV8XXqXoZNE" />
    <preference name="GOOGLE_MAPS_IOS_API_KEY" value="AIzaSyD-T2yF06JC5U_8Lou45QeKVV8XXqXoZNE" />
</widget>
